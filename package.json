{
  "name": "custom",
  "version": "5.1.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-and-deploy-all": "ng build && firebase use debug && firebase deploy --only hosting && ng build --prod && firebase use prod && firebase deploy --only hosting",
    "build-prod-patch": "npm version patch && node ./replace.build.js && ng build --prod --base-href=./ && node ./versioning.build.js",
    "build-prod-no-patch": "node ./replace.build.js && ng build --prod --base-href=./ && node ./versioning.build.js",
    "build-deploy-debug-prod": "npm version patch && node ./replace.build.js && ng build --prod --base-href=/ && node ./versioning.build.js && firebase use debug && firebase deploy --only hosting",
    "build-deploy-debug": "npm version patch && node ./replace.build.js && ng build --base-href=/ && node ./versioning.build.js && firebase use debug-2 && firebase deploy --only hosting"
  },
  "private": true,
